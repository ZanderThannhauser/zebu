

postfix_expression: `['a'-'z']+` #variable;

unary_expression
	: (     "++" #increment
	  |     "--" #decrement
	  |      "&" #address_of
	  |      "*" #dereference
	  |      "+" #positive
	  |      "-" #negative
	  |      "~" #bitwise_not
	  |      "!" #logical_not
	  | "sizeof" #sizeof_)* postfix_expression #expression ;


%start: unary_expression #expression;














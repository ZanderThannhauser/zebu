

%start: root;

%skip: ' ';

highest: `['0'-'9']+` #literal | '(' root #subexpression ')' ;

multiply: highest #term[] (('*' | '/') highest #term[])*;

addition: multiply #term[] (('+' | '-') multiply #term[])*;

// addition: multiply #term[0] (('+' #ops[] | '-' #ops[]) multiply #term[])*;

// addition: multiply #start ('+' multiply #addmes[] | '-' multiply #submes[])*;

root: addition;





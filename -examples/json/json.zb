
%skip: ' ' | '\t' | '\n';

%start: value #value;

`string`: '\"' ['a' - 'z']+ '\"';

keyvalue: `string` #key ":" value #value;

value
	: "true"  #boolean
	| "false" #boolean
	| `['0' - '9']+:"%u"` #number
	| `string` #string
	| "[" value #elements[] ("," value #elements[])* "]"
	| "{" keyvalue #dict[] ("," keyvalue #dict[])* "}"
	;



















